{% extends 'base_backend.html.twig' %}

{% block title %}
    {{ parent() }} -  Inicio
{% endblock %}

{% block body %}

    <div id="top" class="row">
        <div class="page-header">
            <h2>Inicio <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></h2>
        </div>
    </div>



    <div class="row">
        <div class="row">
            <div class="col-md-12">
                {% for type, flashMessages in app.session.flashbag.all() %}
                    {% for flashMessage in flashMessages %}
                        <div class="alert alert-{{ type }}">
                            {{ flashMessage|raw }}
                        </div>
                    {% endfor %}
                {% endfor %}
            </div>
        </div>


    </div>

{% endblock %}
